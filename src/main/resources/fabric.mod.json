{
    "schemaVersion": 1,
    "id": "enderscape",
    "version": "${version}",
    "name": "Enderscape",
    "description": "Total overhaul of The End with new generation, biomes, mobs, and other features.",
    "authors": [
        "lunarbunten",
        "hivye",
        "Konci",
        "Ninni",
        "Orcinus"
    ],
    "contributors": [
        "Aigis (previous logo art)",
        "ArathainFarqoe (additional art)",
        "dopadream (additional sound design)",
        "FourTimesCold (additional concept art)",
        "marleyinbloom (promotional renders and concept art)",
        "Walls (additional structure design and implementation)"
    ],
    "contact": {
        "sources": "https://github.com/lunarbunten/enderscape",
        "issues": "https://github.com/lunarbunten/enderscape/issues"
    },
    "custom": {
        "modmenu": {
            "links": {
                "modmenu.enderscape.curseforge": "https://www.curseforge.com/minecraft/mc-mods/enderscape",
                "modmenu.enderscape.modrinth": "https://modrinth.com/mod/enderscape"
            }
        }
    },
    "license": "MIT",
    "icon": "assets/enderscape/icon.png",
    "environment": "*",
    "entrypoints": {
        "fabric-datagen": [
          "net.bunten.enderscape.datagen.EnderscapeDataGen"
        ],
        "main": [
            "net.bunten.enderscape.Enderscape"
        ],
        "client": [
            "net.bunten.enderscape.client.EnderscapeClient"
        ],
        "modmenu": [
          "net.bunten.enderscape.client.EnderscapeModMenu"
        ]
    },
    "mixins": [
        "enderscape.mixins.json",
        {
            "config": "enderscape.client.mixins.json",
            "environment": "client"
        }
    ],
    "depends": {
        "minecraft": "1.21.1",
        "fabricloader": ">=0.16.9",
        "fabric": ">=0.112.1",
        "mixson": ">=1.3.1",
        "yet_another_config_lib_v3": ">=3.6.2"
    },
    "accessWidener": "enderscape.accesswidener"
}